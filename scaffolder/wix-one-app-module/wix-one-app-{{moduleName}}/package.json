{
  "name": "@wix/wix-one-app-{{moduleName}}",
  "version": "1.0.0",
  "description": "OneApp module template",
  "scripts": {
    "postinstall": "one-app-engine --setup",
    "start": "shono start",
    "pretest": "shono lint && shono tsc && shono testjs",
    "detox:admin": "shono detox -r --app admin",
    "detox:places": "shono detox -r --app places",
    "detox": "npm run detox:admin && npm run detox:places"
  },
  "main": "src/module.ts",
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "url": "https://github.com/wix-private/wix-one-app-{{moduleName}}.git"
  },

  "ciConfig": {
    "parallelJobs": [
      {
        "name": "JS tests",
        "run": "npm run pretest"
      },
      {
        "name": "Detox tests",
        "run": "npm run detox"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@wix/shono": "^8.0.0",
    "wix-one-app-engine": "ga",
    "@wix/wix-one-app-platform": "ga",
    "@wix/wix-one-app-member-view": "ga",
    "@wix/wix-react-native-ui-lib": "ga",
    "@wix/wix-one-app-auth": "ga",
    "@wix/wix-one-app-notifications": "ga",
    "@wix/wix-owners-app-platform": "ga"
  },
  "oneAppEngine": {
    "places": {
      "platformModule": "@wix/wix-one-app-platform",
      "modules": [
        "@wix/wix-one-app-platform",
        "@wix/wix-one-app-auth",
        "@wix/wix-one-app-notifications",
        "@wix/wix-one-app-member-view",
        "@wix/wix-one-app-{{moduleName}}",
        "template-demo-module"
      ],
      "tabs": [
        "demoTab"
      ]
    },
    "admin": {
      "platformModule": "@wix/wix-owners-app-platform",
      "modules": [
        "@wix/wix-owners-app-platform",
        "@wix/wix-one-app-auth",
        "@wix/wix-one-app-notifications",
        "@wix/wix-one-app-{{moduleName}}",
        "template-demo-module"
      ],
      "tabs": [
        "demoTab",
        "manage"
      ]
    }
  }
}
